{
	"version": "0.2.0",
	"configurations": [
	  {
		"name": "Next.js: dev server",
		"type": "node",
		"request": "launch",
		"runtimeExecutable": "npm",
		"runtimeArgs": ["run", "dev"],
		"skipFiles": ["<node_internals>/**"],
		"cwd": "${workspaceFolder}",
		"env": {
		  "NODE_ENV": "development"
		},
		"console": "integratedTerminal",
		"protocol": "inspector",
		"port": 9229
	  },
	  {
		"name": "Attach to Node",
		"type": "node",
		"request": "attach",
		"port": 9229,
		"restart": true,
		"skipFiles": ["<node_internals>/**"]
	  }
	]
  }
  